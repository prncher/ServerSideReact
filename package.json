{
  "name": "serversidereact",
  "version": "0.1.0",
  "description": "Server side react using nodejs and express",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prncher/ServerSideReact.git"
  },
  "main": "index.js",
  "scripts": {
    "cleanBuild": "rimraf ./build/*",
    "node": "node --trace-warnings --use_strict ./server/index.js",
    "start": "react-scripts start",
    "build": "npm run cleanBuild && GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "compression",
    "nodejs",
    "express",
    "react"
  ],
  "author": "Prince Cheruvathur",
  "license": "MIT",
  "dependencies": {
    "compressible": "^2.0.18",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/register": "^7.16.0",
    "ignore-styles": "^5.0.1"
  }
}
